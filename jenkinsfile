pipeline{

 agent any

 stages{
    stage('Build') { 
            steps {
                sh 'npm install'
            }
        }
        stage('Test') { 
            steps {
                sh 'echo "testing the application"'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'echo "deploying the application"'
            }
        }
    }
}
